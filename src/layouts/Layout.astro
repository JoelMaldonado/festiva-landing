---
import "../styles/global.css";
const { title = "Festiva" } = Astro.props;
import "aos/dist/aos.css";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <link rel="icon" href="/favicon.ico" />
  </head>
  <body class="bg-darkB0 scroll-smooth">
    <slot />

    <script>
      import AOS from "aos";
      AOS.init();
    </script>
  </body>
</html>
